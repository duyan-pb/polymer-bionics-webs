{
    "name": "polymer-bionics-website",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "engines": {
        "node": ">=22.12.0"
    },
    "scripts": {
        "dev": "vite",
        "kill": "fuser -k 5000/tcp",
        "build": "npm run typecheck && vite build",
        "build:analyze": "npm run build && npx vite-bundle-visualizer",
        "typecheck": "tsc --noEmit",
        "lint": "eslint . --max-warnings 150",
        "lint:fix": "eslint . --fix",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "test:ui": "vitest --ui",
        "test:mutation": "stryker run",
        "test:mutation:critical": "stryker run --mutate 'src/lib/analytics/consent.ts,src/lib/analytics/identity.ts'",
        "optimize": "vite optimize",
        "preview": "vite preview",
        "clean": "rm -rf dist node_modules/.vite coverage reports .stryker-tmp",
        "type-coverage": "type-coverage --at-least 95 --detail --strict --ignore-files 'src/components/ui/**' --ignore-files 'src/**/__tests__/**'",
        "type-coverage:report": "type-coverage --at-least 95 --detail --strict --json-output > reports/type-coverage.json",
        "knip": "knip",
        "knip:fix": "knip --fix",
        "audit": "npm audit --audit-level=high",
        "audit:fix": "npm audit fix",
        "complexity": "eslint . --format json > reports/eslint-complexity.json || true",
        "quality:report": "npm run test:coverage && npm run type-coverage:report && npm run complexity && npm run knip -- --reporter json > reports/knip.json",
        "validate": "npm run typecheck && npm run lint && npm run test && npm run type-coverage && npm run knip && npm run build",
        "validate:strict": "npm run validate && npm run audit",
        "ci": "npm run validate:strict",
        "precommit": "npm run lint && npm run typecheck && npm run test"
    },
    "dependencies": {
        "@heroicons/react": "^2.2.0",
        "@hookform/resolvers": "^5.2.2",
        "@microsoft/applicationinsights-web": "^3.3.10",
        "@phosphor-icons/react": "^2.1.7",
        "@radix-ui/colors": "^3.0.0",
        "@radix-ui/react-accordion": "^1.2.3",
        "@radix-ui/react-alert-dialog": "^1.1.6",
        "@radix-ui/react-aspect-ratio": "^1.1.2",
        "@radix-ui/react-avatar": "^1.1.3",
        "@radix-ui/react-checkbox": "^1.1.4",
        "@radix-ui/react-collapsible": "^1.1.3",
        "@radix-ui/react-context-menu": "^2.2.6",
        "@radix-ui/react-dialog": "^1.1.6",
        "@radix-ui/react-dropdown-menu": "^2.1.6",
        "@radix-ui/react-hover-card": "^1.1.6",
        "@radix-ui/react-label": "^2.1.2",
        "@radix-ui/react-menubar": "^1.1.6",
        "@radix-ui/react-navigation-menu": "^1.2.5",
        "@radix-ui/react-popover": "^1.1.6",
        "@radix-ui/react-progress": "^1.1.2",
        "@radix-ui/react-radio-group": "^1.2.3",
        "@radix-ui/react-scroll-area": "^1.2.9",
        "@radix-ui/react-select": "^2.1.6",
        "@radix-ui/react-separator": "^1.1.2",
        "@radix-ui/react-slider": "^1.3.6",
        "@radix-ui/react-slot": "^1.1.2",
        "@radix-ui/react-switch": "^1.1.3",
        "@radix-ui/react-tabs": "^1.1.3",
        "@radix-ui/react-toggle": "^1.1.2",
        "@radix-ui/react-toggle-group": "^1.1.2",
        "@radix-ui/react-tooltip": "^1.1.8",
        "@tailwindcss/container-queries": "^0.1.1",
        "@tailwindcss/vite": "^4.1.18",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "cmdk": "^1.1.1",
        "embla-carousel-react": "^8.5.2",
        "framer-motion": "^12.34.0",
        "fuse.js": "^7.1.0",
        "input-otp": "^1.4.2",
        "lucide-react": "^0.563.0",
        "next-themes": "^0.4.6",
        "react": "^19.2.4",
        "react-day-picker": "^9.13.2",
        "react-dom": "^19.2.4",
        "react-error-boundary": "^6.0.0",
        "react-hook-form": "^7.68.0",
        "react-resizable-panels": "^4.6.2",
        "sonner": "^2.0.1",
        "tailwind-merge": "^3.0.2",
        "tw-animate-css": "^1.2.4",
        "vaul": "^1.1.2",
        "zod": "^4.3.6"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "@stryker-mutator/core": "^9.5.1",
        "@stryker-mutator/typescript-checker": "^9.5.1",
        "@stryker-mutator/vitest-runner": "^9.5.1",
        "@tailwindcss/postcss": "^4.1.18",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^16.3.1",
        "@testing-library/user-event": "^14.6.1",
        "@types/react": "^19.2.14",
        "@types/react-dom": "^19.0.4",
        "@vitejs/plugin-react-swc": "^4.2.3",
        "@vitest/coverage-v8": "^4.0.16",
        "cross-env": "^10.1.0",
        "eslint": "^9.39.2",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-react-refresh": "^0.5.0",
        "globals": "^17.3.0",
        "happy-dom": "^20.6.2",
        "jsdom": "^27.3.0",
        "knip": "^5.83.1",
        "sharp": "^0.34.5",
        "tailwindcss": "^4.1.18",
        "type-coverage": "^2.29.7",
        "typescript": "~5.9.3",
        "typescript-eslint": "^8.56.0",
        "vite": "^7.3.0",
        "vitest": "^4.0.16",
        "web-vitals": "^5.1.0"
    }
}
